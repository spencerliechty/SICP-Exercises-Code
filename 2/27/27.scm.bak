(define x (list (list 1 2) (list 3 4)))
(define (reversi x)
	(if (null? (cdr x))
	    (car x)
		(list (reversi (cdr x))
		      (car x))))
(define (drev x)
	(if (null? (cdr x))
		(if (pair? (car x))
			(drev (car x))
			(car x))
		(append (drev (cdr x))
			(if (pair? (car x))
				(drev (car x))
				(car x)))))
